<?xml version="1.0" encoding="UTF-8"?>

<trafficControl id="control_0">

    <!-- speed of vehicle 'veh' changes from 10 m/s to 20 m/s -->
    <speed id="veh" begin="60" end="1000" value="28*(-1)^floor(2(t-60)/200)" maxAccel="2" maxDecel="5" />

</trafficControl>


<trafficControl id="control_1">

    <!-- speed of vehicle 'veh' changes from 10 m/s to 20 m/s -->
    <speed id="veh" begin="60" end="1000" value="(((-1)^floor(2(t-60)/200)+1)*5)+10" maxAccel="1" maxDecel="1" />

</trafficControl>


<trafficControl id="control_2">

    <!-- speed of vehicle 'veh' changes from 5 m/s to 20 m/s -->
    <speed id="veh" begin="60" end="1000" value="(((-1)^floor(2(t-60)/200)+1)*15/2)+5" maxAccel="1.5" maxDecel="2" />

</trafficControl>


<trafficControl id="control_3">

    <!-- speed of vehicle 'veh' changes from 10 m/s to 30 m/s -->
    <speed id="veh" begin="60" end="1000" value="(((-1)^floor(2(t-60)/200)+1)*10)+10" maxAccel="2" maxDecel="5" />

</trafficControl>
